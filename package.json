{
  "name": "react-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/locol23/react-template.git",
  "author": "Yoshitaka Terazawa <locol6622@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "run-s lint && run-p parcel lint-watch",
    "parcel": "parcel src/index.html",
    "lint": "eslint --fix src",
    "lint-watch": "onchange 'src/**/*.js' -- npm run lint",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "recompose": "^0.26.0",
    "styled-components": "^3.2.5"
  },
  "devDependencies": {
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.11.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.5.1",
    "husky": "^0.14.3",
    "lint-staged": "^6.0.1",
    "npm-run-all": "^4.1.2",
    "onchange": "^3.3.0",
    "parcel-bundler": "^1.4.1",
    "prettier": "^1.10.2",
    "prettier-eslint-cli": "^4.7.0"
  }
}
