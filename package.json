{
  "name": "react-template",
  "version": "1.0.0",
  "main": "index.js",
  "repository": "https://github.com/locol23/react-template.git",
  "author": "Yoshitaka Terazawa <locol6622@gmail.com>",
  "license": "MIT",
  "scripts": {
    "clean": "rimraf dist",
    "parcel": "parcel src/index.html",
    "lint": "eslint src",
    "lint-watch": "chokidar src/*.js src/**/*.js -c 'npm run lint'",
    "dev": "run-s clean lint && run-p parcel lint-watch",
    "build": "run-s clean lint && parcel build src/index.html",
    "deploy": "netlify deploy -d dist --prod",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "react": "^16.3.2",
    "react-dom": "^16.2.0",
    "recompose": "^0.30.0",
    "styled-components": "^4.0.0"
  },
  "devDependencies": {
    "@babel/core": "7.1.6",
    "@babel/preset-react": "7.0.0",
    "chokidar-cli": "1.2.1",
    "eslint": "5.9.0",
    "eslint-config-airbnb": "17.1.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-plugin-react": "7.11.1",
    "husky": "0.14.3",
    "lint-staged": "6.1.1",
    "netlify-cli": "2.2.3",
    "npm-run-all": "4.1.5",
    "parcel-bundler": "1.10.3",
    "prettier": "1.15.2",
    "prettier-eslint-cli": "4.7.1",
    "rimraf": "2.6.2"
  }
}
